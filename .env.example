# JETUP Environment Variables Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration (REQUIRED for database)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration (REQUIRED for payments)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_test_publishable_key
VITE_STRIPE_SECRET_KEY=sk_test_your_test_secret_key

# Email Service Configuration (REQUIRED for emails)
SENDGRID_API_KEY=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
SENDGRID_FROM_EMAIL=noreply@yourdomain.com
SENDGRID_FROM_NAME=Your Company Name

# Email Templates (SendGrid Dynamic Template IDs)
SENDGRID_CUSTOMER_WELCOME_TEMPLATE=d-your_template_id
SENDGRID_OPERATOR_WELCOME_TEMPLATE=d-your_template_id
SENDGRID_LOGIN_VERIFICATION_TEMPLATE=d-your_template_id
SENDGRID_PASSWORD_RESET_TEMPLATE=d-your_template_id

# API Configuration
VITE_API_BASE_URL=https://api.yourdomain.com
VITE_API_TIMEOUT=30000

# File Upload Configuration (Optional)
VITE_S3_BUCKET=your_s3_bucket_name
VITE_S3_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Security Configuration
VITE_JWT_SECRET=your_jwt_secret_key_here
VITE_ENCRYPTION_KEY=your_encryption_key_here

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_FACEBOOK_PIXEL_ID=FB_PIXEL_ID

# Feature Flags
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_EMAIL_VERIFICATION=true
VITE_ENABLE_FILE_UPLOADS=true
VITE_MAINTENANCE_MODE=false

# Company Configuration
VITE_COMPANY_NAME=Your Company Name
VITE_SUPPORT_EMAIL=support@yourdomain.com
VITE_SUPPORT_PHONE=+1234567890

# Development/Production Environment
NODE_ENV=development
